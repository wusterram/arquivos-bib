@inproceedings{ WOS:000595855400096,
Author = {Toma, Sorin-George and Marinescu, Paul and Saseanu, Andreea},
Editor = {Chivu, L and IoanFranc, V and Georgescu, G and Andrei, JV},
Title = {Smart Cities for Sustainable Development in the 21st Century},
