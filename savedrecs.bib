@article{ WOS:000486520200005,
Author = {Huang-Lachmann, Jo-Ting},
Title = {Systematic review of smart cities and climate change adaptation},
Journal = {SUSTAINABILITY ACCOUNTING MANAGEMENT AND POLICY JOURNAL},
Year = {2019},
