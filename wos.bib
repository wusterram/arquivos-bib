@article{ WOS:001075124500004,
Author = {Sahinaslan, Onder and Sahin, Ahmet and Sahinaslan, Ender},
Title = {Bibliometric Analysis and Mapping of the Benefits and Challenges of
   Cloud ERP Systems},
