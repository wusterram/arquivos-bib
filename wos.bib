@article{ WOS:000948515100001,
Author = {Moschella, Jason and Boulianne, Emilio and Magnan, Michel},
Title = {Risk Management in Small- and Medium-Sized Businesses and How
   Accountants Contribute{*}},
Journal = {CONTEMPORARY ACCOUNTING RESEARCH},
Year = {2023},
Volume = {40},
Number = {1},
Pages = {668-703},
Month = {MAR},
Abstract = {We investigate how owners of small- and medium-sized enterprises (SMEs)
   perceive, make sense of, and practice risk management. Drawing on
   Schatzki's practice theory, we theorize on how and why risk management
   happens in SMEs. Thus, we fill a gap in the extant literature, which
   focuses almost exclusively on risk management within large
   organizations. We interview entrepreneurs and conduct site observations
   to gain insight into their risk management activities, the drivers that
   lead to the adoption of said activities, their attitudes toward risk
   management, and how their accountants may shape and contribute to risk
   management in SMEs. We find that rather than a specific set of formal
   processes, entrepreneurs view risk management as a mindset that
   emphasizes the preservation of key assets, creation of competitive
   advantages, and development of local talent and expertise. We observe
   practices that are mainly informal yet planned, deliberate, and fully
   integrated within the fabric of organizations that align with ideal
   forms of risk management. We also find that full-time, in-house
   accountants do help entrepreneurs with risk management, while external
   accountants, whose main activities relate to financial statement
   preparation and tax filings, do not systematically help entrepreneurs
   manage risk. We contribute to both the theory and practice of risk
   management by sharing empirical insights into how SME owners perceive,
   make sense of, and manage risk.},
