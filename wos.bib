@article{ WOS:000867086000001,
Author = {Abobakr, Mohamed Ahmed and Abdel-Kader, Magdy and Elbayoumi, Ahmed Fouad},
Title = {Integrating S-ERP systems and lean manufacturing practices to improve
   sustainability performance: an institutional theory perspective},
