@article{ WOS:000648852200006,
Author = {Marques, Katiane Zancan and Behr, Ariel and Malanovicz, Aline Vieira},
Title = {EVALUATION OF THE ACCEPTANCE OF AN ERP SYSTEM IN THE ACCOUNTING
   DEPARTMENT OF A PUBLIC COMPANY ACCORDING TO THE UTAUT MODEL},
Journal = {REVISTA DE GESTAO FINANCAS E CONTABILIDADE},
Year = {2020},
Volume = {10},
Number = {1},
Pages = {82-104},
Month = {JAN-APR},
Abstract = {This research analyses factors and conditions that influence the
   acceptance of an Enterprise Resources Planning (ERP) in the accounting
   department of a public company. The research is descriptive and uses the
   method Qualitative Case Study, adopting the UTAUT model as theoretical
   basis. Three sources were used to collect the data: semi-structured
   interviews, observation and document analysis. The data obtained show
   that the users' expectations were above what was perceived after the
   implementation of the integrated system, generating a frustrating
   sensation and impacting negatively in the usage intentions. This
   research allowed the identification, besides the user perceptions and
   expectations, of some reasons that make them refuse a new technology and
   could risk the high investments made to implement projects of this size.
   Furthermore, it shows that the accounting professional must be prepared
   to get adapted to the companies changes and help the manager to adopt
   solutions that improve the organizational performance.},
