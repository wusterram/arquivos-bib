Title = {Research on quantitative model of ANN-based BOM in coal industry ERP},
Booktitle = {PROCEEDINGS OF THE 14TH INTERNATIONAL CONFERENCE ON INDUSTRIAL
   ENGINEERING AND ENGINEERING MANAGEMENT, VOLS A AND B: BUILDING CORE
   COMPETENCIES THROUGH IE\&EM},
Year = {2007},
