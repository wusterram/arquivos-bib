@inproceedings{ WOS:000261427000055,
Author = {Sheng Ya and Yin Baoxing},
Editor = {Chi, RY},
Title = {Research on Analysis of the Stakeholders in Technological Innovation of
   Complex Products \& Systems: A Case of ERP},
Booktitle = {PROCEEDINGS OF HANGZHOU CONFERENCE ON MANAGEMENT OF TECHNOLOGY (MOT
   2008)},
Year = {2008},
Pages = {325-330},
Note = {Hangzhou Conference on Management of Technology (MOT 2008), Hangzhou,
   PEOPLES R CHINA, MAY 19-21, 2008},
Abstract = {A new field of innovation research is developing around complex product
   systems (CoPS). CoPS are technology-intensive capital goods that forms
   the backbone of modem economy. The research on CoPS has just begun in
   China recently. Prior researches have raised innovation networks of CoPS
   and found related management determinants. Our paper extends prior
   researches by first applying stakeholder theory to technological
   innovation in development of CoPS, We construct the stakeholder analysis
   model in technological innovation of CoPS from two dimensions of power
   and interest to assess their features. Then we use a case of ERP project
   to support the theoretical model. This paper makes a contribution to
   research on stakeholder-based view of innovation management of CoPS.},
Publisher = {ZHEJIANG GONGSHANG UNIV PRESS},
