@inproceedings{ WOS:000375229700003,
Author = {Soja, Ewa and Paliwoda-Pekosz, Grazyna and Soja, Piotr},
Book-Group-Author = {Bucharest Univ Econ Studies},
Title = {PERCEPTION OF DIFFICULTIES DURING ENTERPRISE SYSTEM ADOPTION AND USE:
   THE ROLE OF EMPLOYEES AGE},
Booktitle = {PROCEEDINGS OF THE 10TH INTERNATIONAL CONFERENCE ACCOUNTING AND
   MANAGEMENT INFORMATION SYSTEMS (AMIS 2015)},
Series = {Proceedings of the International Conference Accounting and Management
   Information Systems},
Year = {2015},
Pages = {50-65},
Note = {10th International Conference on Accounting and Management Information
   Systems (AMIS), Bucharest Univ Econ Studies, Bucharest, ROMANIA, JUN
   10-11, 2015},
Abstract = {The paper investigates the influence of employees' age on problem
   perception during enterprise system (ES) implementation and use. In the
   course of exploratory research conducted among Polish companies,
   respondent opinions on ES adoption were gathered. Next, following
   grounded theory approach and incorporating open and axial coding, the
   taxonomy of problems was developed. Then, the distribution of problems
   among different age group was analyzed. The results of our investigation
   revealed that younger employees tend to report problems connected with
   detailed, technical issues. Older employees, on the other hand, seem to
   perceive to a greater extent more general problems, while experiencing
   also problems of technical nature. The current study's findings confirm
   previous results concerning general issues connected with work force
   ageing and, at the same time, shed some new light on ES implementation
   and use.},
